<UserControl x:Class="Core2D.Views.Style.FillStyleView"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:vms="clr-namespace:Core2D.Style;assembly=Core2D"
             xmlns:vmd="clr-namespace:Core2D.Designer;assembly=Core2D"
             mc:Ignorable="d"
             d:DataContext="{x:Static vmd:DesignerContext.Style}"
             d:DesignWidth="250" d:DesignHeight="500"
             x:DataType="vms:FillStyle" x:CompileBindings="True">
    <ContentControl Content="{Binding Color}" />
</UserControl>
