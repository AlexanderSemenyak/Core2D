<UserControl x:Class="Core2D.Views.PresenterView"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:vme="clr-namespace:Core2D.Editor;assembly=Core2D"
             xmlns:vmd="clr-namespace:Core2D.Designer;assembly=Core2D"
             Width="{Binding $self.Container.Width}"
             Height="{Binding $self.Container.Height}"
             Focusable="True"
             mc:Ignorable="d"
             d:DataContext="{x:Static vmd:DesignerContext.Editor}"
             d:DesignWidth="1200" d:DesignHeight="540"
             x:DataType="vme:ProjectEditor" x:CompileBindings="False">
</UserControl>
