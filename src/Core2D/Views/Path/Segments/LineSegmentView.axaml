<UserControl x:Class="Core2D.Views.Path.Segments.LineSegmentView"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:vmps="using:Core2D.ViewModels.Path.Segments"
             xmlns:vmd="using:Core2D.ViewModels.Designer"
             mc:Ignorable="d"
             d:DataContext="{x:Static vmd:DesignerContext.LineSegment}"
             d:DesignWidth="250" d:DesignHeight="400"
             x:DataType="vmps:LineSegmentViewModel" x:CompileBindings="True">
    <StackPanel>
        <Expander Header="Point" IsExpanded="False">
            <ContentControl Content="{Binding Point}" />
        </Expander>
    </StackPanel>
</UserControl>
