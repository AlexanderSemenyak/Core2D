<UserControl x:Class="Core2D.Views.Path.Segments.CubicBezierSegmentView"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:vmps="using:Core2D.ViewModels.Path.Segments"
             xmlns:vmd="using:Core2D.ViewModels.Designer"
             mc:Ignorable="d"
             d:DataContext="{x:Static vmd:DesignerContext.CubicBezierSegment}"
             d:DesignWidth="250" d:DesignHeight="400"
             x:DataType="vmps:CubicBezierSegmentViewModel" x:CompileBindings="True">
  <TabControl>
    <TabItem Header="Point1">
      <ContentControl Content="{Binding Point1}" />
    </TabItem>
    <TabItem Header="Point2">
      <ContentControl Content="{Binding Point2}" />
    </TabItem>
    <TabItem Header="Point3">
      <ContentControl Content="{Binding Point3}" />
    </TabItem>
  </TabControl>
</UserControl>
