                  ToolTip.Tip="Union (inclusive-or) the two paths."
                  Command="{Binding ShapeService.OnPathOp}" 
                  CommandParameter="Union" 
                  InputGesture="Ctrl+Shift+U" />
        <MenuItem Header="_Xor"
                  Icon="{StaticResource XorIcon}"
                  ToolTip.Tip="Exclusive-or the two paths."
                  Command="{Binding ShapeService.OnPathOp}"
                  CommandParameter="Xor"
                  InputGesture="Ctrl+Shift+X" />
        <MenuItem Header="_Reverse Difference" 
                  Icon="{StaticResource ReverseDifferenceIcon}"
                  ToolTip.Tip="Subtract the current path from the op path."
                  Command="{Binding ShapeService.OnPathOp}" 
                  CommandParameter="ReverseDifference" InputGesture="Ctrl+Shift+R" />
      </MenuItem>
