<UserControl x:Class="Core2D.Views.Docking.Views.HomeMenuView"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:converters="using:Core2D.Converters"
             xmlns:app="using:Core2D"
             xmlns:vm="using:Core2D.ViewModels.Docking.Views"
             xmlns:vme="using:Core2D.ViewModels.Editor"
             xmlns:vmd="using:Core2D.ViewModels.Designer"
             mc:Ignorable="d"
             d:DesignWidth="800" d:DesignHeight="32"
             x:DataType="vm:HomeMenuViewModel" x:CompileBindings="True">
  <UserControl.Resources>
    <!-- Menu-Shape -->
    <Image x:Key="DifferenceIcon" Source="{StaticResource Difference}" Width="16" Height="16" />
    <Image x:Key="IntersectIcon" Source="{StaticResource Intersect}" Width="16" Height="16" />
    <Image x:Key="UnionIcon" Source="{StaticResource Union}" Width="16" Height="16" />
    <Image x:Key="XorIcon" Source="{StaticResource Xor}" Width="16" Height="16" />
    <Image x:Key="ReverseDifferenceIcon" Source="{StaticResource ReverseDifference}" Width="16" Height="16" />
    <!-- Menu-Tool -->
    <Image x:Key="NoneIcon" Source="{StaticResource None}" Width="16" Height="16" />
    <Image x:Key="SelectionIcon" Source="{StaticResource Selection}" Width="16" Height="16" />
    <Image x:Key="PointIcon" Source="{StaticResource Point}" Width="16" Height="16" />
    <Image x:Key="LineIcon" Source="{StaticResource Line}" Width="16" Height="16" />
    <Image x:Key="ArcIcon" Source="{StaticResource Arc}" Width="16" Height="16" />
    <Image x:Key="CubicBezierIcon" Source="{StaticResource CubicBezier}" Width="16" Height="16" />
    <Image x:Key="QuadraticBezierIcon" Source="{StaticResource QuadraticBezier}" Width="16" Height="16" />
    <Image x:Key="PathIcon" Source="{StaticResource Path}" Width="16" Height="16" />
    <Image x:Key="PathLineIcon" Source="{StaticResource Line}" Width="16" Height="16" />
    <Image x:Key="PathArcIcon" Source="{StaticResource Arc}" Width="16" Height="16" />
    <Image x:Key="PathCubicBezierIcon" Source="{StaticResource CubicBezier}" Width="16" Height="16" />
    <Image x:Key="PathQuadraticBezierIcon" Source="{StaticResource QuadraticBezier}" Width="16" Height="16" />
    <Image x:Key="PathMoveIcon" Source="{StaticResource Move}" Width="16" Height="16" />
    <Image x:Key="RectangleIcon" Source="{StaticResource Rectangle}" Width="16" Height="16" />
    <Image x:Key="EllipseIcon" Source="{StaticResource Ellipse}" Width="16" Height="16" />
    <Image x:Key="CircleIcon" Source="{StaticResource Circle}" Width="16" Height="16" />
    <Image x:Key="TextIcon" Source="{StaticResource Text}" Width="16" Height="16" />
    <Image x:Key="ImageIcon" Source="{StaticResource Image}" Width="16" Height="16" />
  </UserControl.Resources>
  <UserControl.Styles>
    <Style Selector="Separator.vertical">
      <Setter Property="VerticalAlignment" Value="Center" />
      <Setter Property="Margin" Value="2,0,2,0" />
      <Setter Property="Background" Value="{DynamicResource SystemChromeAltLowColor}" />
      <Setter Property="Opacity" Value="0.3" />
      <Setter Property="Width" Value="1" />
      <Setter Property="Height" Value="24" />
    </Style>
    <Style Selector="RadioButton.tool">
      <Setter Property="Template">
        <Setter.Value>
          <ControlTemplate TargetType="RadioButton">
            <Panel Name="PART_Panel" Width="24" Height="24" Margin="2,0,2,0">
              <Image Source="{TemplateBinding Tag}" Width="16" Height="16" HorizontalAlignment="Center" VerticalAlignment="Center" />
            </Panel>
          </ControlTemplate>
        </Setter.Value>
      </Setter>
    </Style>
    <Style Selector="RadioButton.tool /template/ Panel#PART_Panel">
      <Setter Property="Background" Value="{DynamicResource MenuFlyoutItemBackground}" />
    </Style>
    <Style Selector="RadioButton.tool:checked /template/ Panel#PART_Panel">
      <Setter Property="Background" Value="{DynamicResource MenuFlyoutItemBackgroundPointerOver}" />
    </Style>
    <Style Selector="RadioButton.tool:pressed /template/ Panel#PART_Panel">
      <Setter Property="Background" Value="{DynamicResource MenuFlyoutItemBackgroundPressed}" />
    </Style>
    <Style Selector="RadioButton.tool:pointerover /template/ Panel#PART_Panel">
      <Setter Property="Background" Value="{DynamicResource MenuFlyoutItemBackgroundPressed}" />
    </Style>
    <Style Selector="RadioButton.tool:disabled /template/ Panel#PART_Panel">
      <Setter Property="Background" Value="{DynamicResource MenuFlyoutItemBackgroundDisabled}" />
    </Style>
  </UserControl.Styles>
  <DockPanel DataContext="{Binding Context}"
             LastChildFill="False"
             HorizontalAlignment="Left">
    <Menu d:DataContext="{x:Static vmd:DesignerContext.Editor}" 
          x:DataType="vme:ProjectEditorViewModel" x:CompileBindings="False"
          DockPanel.Dock="Left">
      <MenuItem Header="_File">
        <MenuItem Header="_New" Command="{Binding OnNew}" CommandParameter="{Binding}" InputGesture="Ctrl+N" />
        <Separator />
        <MenuItem Header="_Open..." Command="{Binding Platform.OnOpen}" InputGesture="Ctrl+O" />
        <Separator />
        <MenuItem Header="_Close" Command="{Binding OnCloseProject}" InputGesture="Ctrl+F4" />
        <Separator />
        <MenuItem Header="_Save" Command="{Binding Platform.OnSave}" InputGesture="Ctrl+S" />
        <MenuItem Header="Save _As..." Command="{Binding Platform.OnSaveAs}" InputGesture="Ctrl+Shift+S" />
        <Separator />
        <MenuItem Header="Execu_te Script..." Command="{Binding Platform.OnExecuteScriptFile}" />
        <Separator />
        <MenuItem Header="Import _Json..." Command="{Binding Platform.OnImportJson}" />
        <Separator />
        <MenuItem Header="Import Sv_g..." Command="{Binding Platform.OnImportSvg}" />
        <Separator />
        <MenuItem Header="_Export..." Command="{Binding Platform.OnExport}" InputGesture="Ctrl+E" />
        <Separator />
        <MenuItem Header="_Recent" Items="{Binding RecentProjects}">
          <MenuItem.Styles>
            <Style Selector="MenuItem">
              <Setter Property="Header" Value="{Binding Name}" />
              <Setter Property="Command" Value="{Binding $parent[TopLevel].DataContext.Platform.OnOpen}" />
              <Setter Property="CommandParameter" Value="{Binding Path}" />
            </Style>
          </MenuItem.Styles>
        </MenuItem>
        <Separator />
        <MenuItem Header="E_xit" Command="{Binding Platform.OnExit}" InputGesture="Alt+F4" />
      </MenuItem>
      <MenuItem Header="_Edit">
        <MenuItem Header="_Undo" Command="{Binding OnUndo}" InputGesture="Ctrl+Z" />
        <MenuItem Header="_Redo" Command="{Binding OnRedo}" InputGesture="Ctrl+Y" />
        <Separator />
        <MenuItem Header="Cu_t" Command="{Binding ClipboardService.OnCut}" InputGesture="Ctrl+X" />
        <MenuItem Header="_Copy" Command="{Binding ClipboardService.OnCopy}" InputGesture="Ctrl+C" />
        <MenuItem Header="_Paste" Command="{Binding ClipboardService.OnPaste}" InputGesture="Ctrl+V" />
        <MenuItem Header="_Delete" Command="{Binding ClipboardService.OnDelete}" InputGesture="Delete" />
        <Separator />
        <MenuItem Header="Copy As _Xaml" Command="{Binding Platform.OnCopyAsXaml}" />
        <Separator />
        <MenuItem Header="Copy As _Svg" Command="{Binding Platform.OnCopyAsSvg}" />
        <MenuItem Header="Paste Sv_g" Command="{Binding Platform.OnPasteSvg}" />
        <Separator />
        <MenuItem Header="Copy As _Metafile" Command="{Binding Platform.OnCopyAsEmf}" />
        <Separator />
        <MenuItem Header="Copy As Svg Path Data" Command="{Binding Platform.OnCopyAsPathData}" />
        <MenuItem Header="Paste Stroked Svg Path Data" Command="{Binding Platform.OnPastePathDataStroked}" />
        <MenuItem Header="Paste Filled Svg Path Data" Command="{Binding Platform.OnPastePathDataFilled}" />
        <Separator />
        <MenuItem Header="Select _All" Command="{Binding SelectionService.OnSelectAll}" InputGesture="Ctrl+A" />
        <MenuItem Header="De_select All" Command="{Binding SelectionService.OnDeselectAll}" InputGesture="Ctrl+Shift+A" />
        <Separator />
        <MenuItem Header="Cl_ear All" Command="{Binding SelectionService.OnClearAll}" />
        <Separator />
        <MenuItem Header="Cance_l" Command="{Binding OnCancel}" InputGesture="Escape" />
      </MenuItem>
      <MenuItem Header="_View">
        <MenuItem Header="_Tool Windows">
          <MenuItem Header="Project _Explorer" Command="{Binding OnToggleDockableVisibility}" CommandParameter="ProjectExplorer" />
          <Separator />
          <MenuItem Header="Page Properties" Command="{Binding OnToggleDockableVisibility}" CommandParameter="PageProperties" />
          <MenuItem Header="Shape Properties" Command="{Binding OnToggleDockableVisibility}" CommandParameter="ShapeProperties" />
          <MenuItem Header="Style Properties" Command="{Binding OnToggleDockableVisibility}" CommandParameter="StyleProperties" />
          <MenuItem Header="Data Properties" Command="{Binding OnToggleDockableVisibility}" CommandParameter="DataProperties" />
          <MenuItem Header="State Properties" Command="{Binding OnToggleDockableVisibility}" CommandParameter="StateProperties" />
          <Separator />
          <MenuItem Header="Style Library" Command="{Binding OnToggleDockableVisibility}" CommandParameter="StyleLibrary" />
          <MenuItem Header="Group Library" Command="{Binding OnToggleDockableVisibility}" CommandParameter="GroupLibrary" />
          <MenuItem Header="Database Library" Command="{Binding OnToggleDockableVisibility}" CommandParameter="DatabaseLibrary" />
          <MenuItem Header="Template Library" Command="{Binding OnToggleDockableVisibility}" CommandParameter="TemplateLibrary" />
          <MenuItem Header="Script Library" Command="{Binding OnToggleDockableVisibility}" CommandParameter="ScriptLibrary" />
          <Separator />
          <MenuItem Header="Project Options" Command="{Binding OnToggleDockableVisibility}" CommandParameter="ProjectOptions" />
          <MenuItem Header="Renderer Options" Command="{Binding OnToggleDockableVisibility}" CommandParameter="RendererOptions" />
          <MenuItem Header="Zoom Options" Command="{Binding OnToggleDockableVisibility}" CommandParameter="ZoomOptions" />
          <MenuItem Header="Image Options" Command="{Binding OnToggleDockableVisibility}" CommandParameter="ImageOptions" />
          <Separator />
          <MenuItem Header="Object Browser" Command="{Binding OnToggleDockableVisibility}" CommandParameter="ObjectBrowser" />
        </MenuItem>
        <MenuItem Header="_Appearance">
          <MenuItem Header="_Main Menu" Command="{Binding OnToggleDockableVisibility}" CommandParameter="HomeMenuView" />
          <MenuItem Header="_Status Bar" Command="{Binding OnToggleDockableVisibility}" CommandParameter="HomeStatusBarView" />
        </MenuItem>
        <MenuItem Header="_Zoom">
          <MenuItem Header="_Actual Size" Command="{Binding Platform.OnZoomReset}" InputGesture="Ctrl+D0" />
          <MenuItem Header="Fit _Page" Command="{Binding Platform.OnZoomUniform}" InputGesture="Ctrl+D1" />
          <MenuItem Header="Fit To Fi_ll" Command="{Binding Platform.OnZoomUniformToFill}" InputGesture="Ctrl+D2" />
          <MenuItem Header="_Fill Page" Command="{Binding Platform.OnZoomFill}" InputGesture="Ctrl+D3" />
          <MenuItem Header="A_uto Fit Page" Command="{Binding Platform.OnZoomAutoFit}" InputGesture="Ctrl+D4" />
          <Separator />
          <MenuItem Header="Zoom _In" Command="{Binding Platform.OnZoomIn}" InputGesture="Ctrl+Add" />
          <MenuItem Header="Zoom _Out" Command="{Binding Platform.OnZoomOut}" InputGesture="Ctrl+Subtract" />
        </MenuItem>
        <MenuItem Header="T_heme">
          <MenuItem Header="Fluent _Light" Command="{x:Static app:App.ChangeTheme}" CommandParameter="FluentLight" />
          <MenuItem Header="Fluent _Dark" Command="{x:Static app:App.ChangeTheme}" CommandParameter="FluentDark" />
          <Separator />
          <MenuItem Header="Default L_ight" Command="{x:Static app:App.ChangeTheme}" CommandParameter="DefaultLight" />
          <MenuItem Header="Default D_ark" Command="{x:Static app:App.ChangeTheme}" CommandParameter="DefaultDark" />
        </MenuItem>
      </MenuItem>
      <MenuItem Header="_Project">
        <MenuItem Header="_Document">
          <MenuItem Header="_Add Document" Command="{Binding OnAddDocument}" CommandParameter="{Binding Mode=OneWay}" />
          <Separator />
          <MenuItem Header="Insert Document Bef_ore" Command="{Binding OnInsertDocumentBefore}" CommandParameter="{Binding Project.CurrentDocument}" />
          <MenuItem Header="Insert Document Aft_er" Command="{Binding OnInsertDocumentAfter}" CommandParameter="{Binding Project.CurrentDocument}" />
          <Separator />
          <MenuItem Header="Cu_t Document" Command="{Binding ClipboardService.OnCut}" CommandParameter="{Binding Project.CurrentDocument}" />
          <MenuItem Header="_Copy Document" Command="{Binding ClipboardService.OnCopy}" CommandParameter="{Binding Project.CurrentDocument}" />
          <MenuItem Header="_Paste Document" Command="{Binding ClipboardService.OnPaste}" CommandParameter="{Binding Project.CurrentDocument}" />
          <Separator />
          <MenuItem Header="_Delete Document" Command="{Binding ClipboardService.OnDelete}" CommandParameter="{Binding Project.CurrentDocument}" />
          <Separator />
          <MenuItem Header="E_xport Document" Command="{Binding Platform.OnExport}" CommandParameter="{Binding Project.CurrentDocument}" />
        </MenuItem>
        <MenuItem Header="P_age">
          <MenuItem Header="_Add Page" Command="{Binding OnAddPage}" CommandParameter="{Binding Mode=OneWay}" />
          <Separator />
          <MenuItem Header="Insert Page Bef_ore" Command="{Binding OnInsertPageBefore}" CommandParameter="{Binding Project.CurrentContainer}" />
          <MenuItem Header="Insert Page Aft_er"  Command="{Binding OnInsertPageAfter}" CommandParameter="{Binding Project.CurrentContainer}" />
          <Separator />
          <MenuItem Header="Cu_t Page" Command="{Binding ClipboardService.OnCut}" CommandParameter="{Binding Project.CurrentContainer}" />
          <MenuItem Header="_Copy Page" Command="{Binding ClipboardService.OnCopy}" CommandParameter="{Binding Project.CurrentContainer}" />
          <MenuItem Header="_Paste Page" Command="{Binding ClipboardService.OnPaste}" CommandParameter="{Binding Project.CurrentContainer}" />
          <Separator />
          <MenuItem Header="_Delete Page" Command="{Binding ClipboardService.OnDelete}" CommandParameter="{Binding Project.CurrentContainer}" />
          <Separator />
          <MenuItem Header="E_xport Page" Command="{Binding Platform.OnExport}" CommandParameter="{Binding Project.CurrentContainer}" />
        </MenuItem>
        <MenuItem Header="_Layer">
          <MenuItem Header="_Add Layer" Command="{Binding OnAddLayer}" CommandParameter="{Binding Project.CurrentContainer}" />
          <Separator />
          <MenuItem Header="_Delete Layer" Command="{Binding OnRemoveLayer}" CommandParameter="{Binding Project.CurrentContainer.CurrentLayer}" />
        </MenuItem>
        <MenuItem Header="D_ata">
          <MenuItem Header="_Import Database..." 
                    Command="{Binding Platform.OnImportData}" 
                    CommandParameter="{Binding Project}" />
          <Separator />
          <MenuItem Header="_Export Database..." 
                    Command="{Binding Platform.OnExportData}"
                    CommandParameter="{Binding Project.CurrentDatabase}" />
          <Separator />
          <MenuItem Header="_Update Database..." 
                    Command="{Binding Platform.OnUpdateData}" 
                    CommandParameter="{Binding Project.CurrentDatabase}" />
        </MenuItem>
        <MenuItem Header="O_ptions">
          <MenuItem Header="Default is Stro_ked" Command="{Binding OnToggleDefaultIsStroked}" InputGesture="K">
            <MenuItem.Icon>
              <CheckBox IsChecked="{Binding Project.Options.DefaultIsStroked}" />
            </MenuItem.Icon>
          </MenuItem>
          <MenuItem Header="Default is _Filled" Command="{Binding OnToggleDefaultIsFilled}" InputGesture="F">
            <MenuItem.Icon>
              <CheckBox IsChecked="{Binding Project.Options.DefaultIsFilled}" />
            </MenuItem.Icon>
          </MenuItem>
          <Separator />
          <MenuItem Header="Default is Close_d" Command="{Binding OnToggleDefaultIsClosed}" InputGesture="D">
            <MenuItem.Icon>
              <CheckBox IsChecked="{Binding Project.Options.DefaultIsClosed}" />
            </MenuItem.Icon>
          </MenuItem>
          <Separator />
          <MenuItem Header="Snap to _Grid" Command="{Binding OnToggleSnapToGrid}" InputGesture="G">
            <MenuItem.Icon>
              <CheckBox IsChecked="{Binding Project.Options.SnapToGrid}" />
            </MenuItem.Icon>
          </MenuItem>
          <Separator />
          <MenuItem Header="Try To _Connect" Command="{Binding OnToggleTryToConnect}" InputGesture="C">
            <MenuItem.Icon>
              <CheckBox IsChecked="{Binding Project.Options.TryToConnect}" />
            </MenuItem.Icon>
          </MenuItem>
        </MenuItem>
        <Separator />
        <MenuItem Header="E_xport Project" Command="{Binding Platform.OnExport}" CommandParameter="{Binding Project}" />
      </MenuItem>
      <MenuItem Header="_Shape">
        <MenuItem Header="Du_plicate" Command="{Binding ShapeService.OnDuplicateSelected}" InputGesture="Ctrl+D" />
        <Separator />
        <MenuItem Header="_Group">
          <MenuItem Header="_Group" Command="{Binding ShapeService.OnGroupSelected}" InputGesture="Ctrl+G" />
          <MenuItem Header="U_ngroup" Command="{Binding ShapeService.OnUngroupSelected}" InputGesture="Ctrl+U" />
        </MenuItem>
        <MenuItem Header="Rotat_e">
          <MenuItem Header="Rotate _Right 90°" Command="{Binding ShapeService.OnRotateSelected}" CommandParameter="90" InputGesture="Ctrl+R" />
          <MenuItem Header="Rotate _Left 90°" Command="{Binding ShapeService.OnRotateSelected}" CommandParameter="-90" InputGesture="Ctrl+L" />
          <MenuItem Header="Flip _Horizontal" Command="{Binding ShapeService.OnFlipHorizontalSelected}" InputGesture="Ctrl+H" />
          <MenuItem Header="Flip _Vertical" Command="{Binding ShapeService.OnFlipVerticalSelected}" InputGesture="Ctrl+J" />
        </MenuItem>
        <MenuItem Header="_Move">
          <MenuItem Header="_Up" Command="{Binding ShapeService.OnMoveUpSelected}" InputGesture="Up" />
          <MenuItem Header="_Down" Command="{Binding ShapeService.OnMoveDownSelected}" InputGesture="Down" />
          <MenuItem Header="_Left" Command="{Binding ShapeService.OnMoveLeftSelected}" InputGesture="Left" />
          <MenuItem Header="_Right" Command="{Binding ShapeService.OnMoveRightSelected}" InputGesture="Right" />
        </MenuItem>
        <MenuItem Header="Stac_k">
          <MenuItem Header="_Horizontally" Command="{Binding ShapeService.OnStackHorizontallySelected}" InputGesture="Shift+H" />
          <MenuItem Header="_Vertically" Command="{Binding ShapeService.OnStackVerticallySelected}" InputGesture="Shift+Y" />
        </MenuItem>
        <MenuItem Header="Dis_tribute">
          <MenuItem Header="_Horizontally" Command="{Binding ShapeService.OnDistributeHorizontallySelected}" InputGesture="Ctrl+Shift+H" />
          <MenuItem Header="_Vertically" Command="{Binding ShapeService.OnDistributeVerticallySelected}" InputGesture="Ctrl+Shift+Y" />
        </MenuItem>
        <MenuItem Header="Alig_n">
          <MenuItem Header="_Left" Command="{Binding ShapeService.OnAlignLeftSelected}" InputGesture="Ctrl+Shift+Left" />
          <MenuItem Header="_Centered" Command="{Binding ShapeService.OnAlignCenteredSelected}" InputGesture="Ctrl+Shift+Subtract" />
          <MenuItem Header="_Right" Command="{Binding ShapeService.OnAlignRightSelected}" InputGesture="Ctrl+Shift+Right" />
          <Separator />
          <MenuItem Header="_Top" Command="{Binding ShapeService.OnAlignTopSelected}" InputGesture="Ctrl+Shift+Up" />
          <MenuItem Header="C_enter" Command="{Binding ShapeService.OnAlignCenterSelected}" InputGesture="Ctrl+Shift+OemPipe" />
          <MenuItem Header="_Bottom" Command="{Binding ShapeService.OnAlignBottomSelected}" InputGesture="Ctrl+Shift+Down" />
        </MenuItem>
        <MenuItem Header="_Arange">
          <MenuItem Header="_Bring to Front" Command="{Binding ShapeService.OnBringToFrontSelected}" InputGesture="Ctrl+Shift+OemCloseBrackets" />
          <MenuItem Header="Bring _Forward" Command="{Binding ShapeService.OnBringForwardSelected}" InputGesture="Ctrl+OemCloseBrackets" />
          <Separator />
          <MenuItem Header="Send Back_ward" Command="{Binding ShapeService.OnSendBackwardSelected}" InputGesture="Ctrl+OemOpenBrackets" />
          <MenuItem Header="_Send to Back" Command="{Binding ShapeService.OnSendToBackSelected}" InputGesture="Ctrl+Shift+OemOpenBrackets" />
        </MenuItem>
        <Separator />
        <MenuItem Header="Create Pat_h" Command="{Binding ShapeService.OnCreatePath}" />
        <MenuItem Header="Create Str_oke Path" Command="{Binding ShapeService.OnCreateStrokePath}" />
        <MenuItem Header="Create _Fill Path" Command="{Binding ShapeService.OnCreateFillPath}" />
        <MenuItem Header="Create _Winding Path" Command="{Binding ShapeService.OnCreateWindingPath}" />
        <MenuItem Header="_Simplify Path" Command="{Binding ShapeService.OnPathSimplify}" />
        <MenuItem Header="_Break Path" Command="{Binding ShapeService.OnPathBreak}" InputGesture="Ctrl+B" />
        <Separator />
        <MenuItem Header="_Difference" 
                  Icon="{StaticResource DifferenceIcon}" 
                  ToolTip.Tip="Subtract the op path from the current path." 
                  Command="{Binding ShapeService.OnPathOp}" 
                  CommandParameter="Difference" 
                  InputGesture="Ctrl+Shift+D" />
        <MenuItem Header="_Intersect" 
                  Icon="{StaticResource IntersectIcon}" 
                  ToolTip.Tip="Intersect the two paths."
                  Command="{Binding ShapeService.OnPathOp}" 
                  CommandParameter="Intersect" 
                  InputGesture="Ctrl+Shift+I" />
