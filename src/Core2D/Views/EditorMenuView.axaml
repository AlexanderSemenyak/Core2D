                        </MenuItem.Icon>
                    </MenuItem>
                    <MenuItem Header="Default is _Filled" Command="{Binding OnToggleDefaultIsFilled}" InputGesture="F">
                        <MenuItem.Icon>
                            <CheckBox IsChecked="{Binding Project.Options.DefaultIsFilled}" />
                        </MenuItem.Icon>
                    </MenuItem>
                    <Separator />
                    <MenuItem Header="Default is Close_d" Command="{Binding OnToggleDefaultIsClosed}" InputGesture="D">
                        <MenuItem.Icon>
                            <CheckBox IsChecked="{Binding Project.Options.DefaultIsClosed}" />
                        </MenuItem.Icon>
                    </MenuItem>
                    <Separator />
                    <MenuItem Header="Snap to _Grid" Command="{Binding OnToggleSnapToGrid}" InputGesture="G">
                        <MenuItem.Icon>
                            <CheckBox IsChecked="{Binding Project.Options.SnapToGrid}" />
                        </MenuItem.Icon>
                    </MenuItem>
                    <Separator />
                    <MenuItem Header="Try To _Connect" Command="{Binding OnToggleTryToConnect}" InputGesture="C">
                        <MenuItem.Icon>
                            <CheckBox IsChecked="{Binding Project.Options.TryToConnect}" />
                        </MenuItem.Icon>
                    </MenuItem>
                </MenuItem>
                <Separator />
                <MenuItem Header="E_xport Project" Command="{Binding Platform.OnExport}" CommandParameter="{Binding Project}" />
            </MenuItem>
            <MenuItem Header="_Document">
                <MenuItem Header="_Add Document" Command="{Binding OnAddDocument}" CommandParameter="{Binding Mode=OneWay}" />
                <Separator />
                <MenuItem Header="Insert Document Bef_ore" Command="{Binding OnInsertDocumentBefore}" CommandParameter="{Binding Project.CurrentDocument}" />
                <MenuItem Header="Insert Document Aft_er" Command="{Binding OnInsertDocumentAfter}" CommandParameter="{Binding Project.CurrentDocument}" />
                <Separator />
                <MenuItem Header="Cu_t Document" Command="{Binding OnCut}" CommandParameter="{Binding Project.CurrentDocument}" />
