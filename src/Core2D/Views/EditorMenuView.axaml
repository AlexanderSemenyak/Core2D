                    <MenuItem Header="_Send to Back" Command="{Binding OnSendToBackSelected}" InputGesture="Ctrl+Shift+OemOpenBrackets" />
                </MenuItem>
                <Separator />
                <MenuItem Header="Create Pat_h" Command="{Binding OnCreatePath}" />
                <MenuItem Header="Create Str_oke Path" Command="{Binding OnCreateStrokePath}" />
                <MenuItem Header="Create _Fill Path" Command="{Binding OnCreateFillPath}" />
                <MenuItem Header="Create _Winding Path" Command="{Binding OnCreateWindingPath}" />
                <MenuItem Header="_Simplify Path" Command="{Binding OnPathSimplify}" />
                <MenuItem Header="_Break Path" Command="{Binding OnPathBreak}" InputGesture="Ctrl+B" />
                <Separator />
                <MenuItem Header="_Difference" Icon="{StaticResource DifferenceIcon}" ToolTip.Tip="Subtract the op path from the current path." Command="{Binding OnPathOp}" CommandParameter="Difference" InputGesture="Ctrl+Shift+D" />
                <MenuItem Header="_Intersect" Icon="{StaticResource IntersectIcon}" ToolTip.Tip="Intersect the two paths." Command="{Binding OnPathOp}" CommandParameter="Intersect" InputGesture="Ctrl+Shift+I" />
                <MenuItem Header="_Union" Icon="{StaticResource UnionIcon}" ToolTip.Tip="Union (inclusive-or) the two paths." Command="{Binding OnPathOp}" CommandParameter="Union" InputGesture="Ctrl+Shift+U" />
                <MenuItem Header="_Xor" Icon="{StaticResource XorIcon}" ToolTip.Tip="Exclusive-or the two paths." Command="{Binding OnPathOp}" CommandParameter="Xor" InputGesture="Ctrl+Shift+X" />
                <MenuItem Header="_Reverse Difference" Icon="{StaticResource ReverseDifferenceIcon}" ToolTip.Tip="Subtract the current path from the op path." Command="{Binding OnPathOp}" CommandParameter="ReverseDifference" InputGesture="Ctrl+Shift+R" />
            </MenuItem>
